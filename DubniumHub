-- Correct Rayfield Loadstring
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Create the Rayfield Window
local Window = Rayfield:CreateWindow({
    Name = "DubniumHub🤖", -- Title of the window
    LoadingTitle = "Loading Hub...",
    LoadingSubtitle = "by dbao🎿, Discord:dubnium1604",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = nil,
        FileName = "RayfieldHub"
    },
    KeySystem = false, -- Optional key system
    Key = "YourKeyHere" -- Optional key if using key system
})

-- Create a Tab
local MainTab = Window:CreateTab("Main", 4483362458) -- You can use an icon ID here

-- Add Button for InventoryESP
MainTab:CreateButton({
    Name = "Inventory ESP🎒", -- Button text
    Callback = function()
        -- Execute the InventoryESP script
        loadstring(game:HttpGet("https://raw.githubusercontent.com/dubnium1604/DubniumHub/refs/heads/main/InventoryESP"))()
    end
})

-- Add Button for Instant Interact
